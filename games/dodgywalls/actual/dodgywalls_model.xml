        <name>click()</name>
        <timeelapsed>2</timeelapsed>
        <effect>(previousball.direction != ball.direction) or (ball.yCoordinate&lt;185) or (ball.yCoordinate > 325 - 12)</effect>
        <frequency_of_use>0.05</frequency_of_use>
    </actions>
