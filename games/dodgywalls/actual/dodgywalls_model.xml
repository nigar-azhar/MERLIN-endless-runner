<modelClass>
    <actions>
        <key>-</key>
        <name>doNothing()</name>
        <timeelapsed>1</timeelapsed>
        <effect>(previousball.direction == ball.direction and ((ball.direction=="up" and ball.yCoordinate &lt;= previousball.yCoordinate - 3 and ball.yCoordinate &gt;= previousball.yCoordinate - 5) or (ball.direction=="down" and ball.yCoordinate >= previousball.yCoordinate + 3 and ball.yCoordinate &lt;= previousball.yCoordinate + 5))) or (ball.yCoordinate&lt;185) or (ball.yCoordinate > 325 - 12)</effect>
